{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/success.html",
      "dest": "/src/public/success.html"
    },
    {
      "src": "/cancel.html",
      "dest": "/src/public/cancel.html"
    },
    {
      "src": "/",
      "dest": "/src/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
